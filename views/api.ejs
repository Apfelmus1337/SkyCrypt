                    </div>
                </div>
            </div>
        </div>


    
        <script src="resources/js/jsonview.bundle.js"></script>
        <script type="text/javascript">
            fetch('resources/api/profile.json')
            .then((res)=> {
                return res.text();
            })
            .then((data) => {
                const tree = JsonView.createTree(data);
                JsonView.render(tree, document.querySelector('#response-profile'));
            })
            .catch((err) => {
                console.log(err);
            });

            fetch('resources/api/talismans_profile.json')
            .then((res)=> {
                return res.text();
            })
            .then((data) => {
                const tree = JsonView.createTree(data);
                JsonView.render(tree, document.querySelector('#response-talismans-profile'));
            })
            .catch((err) => {
                console.log(err);
            });

            fetch('resources/api/talismans.json')
            .then((res)=> {
                return res.text();
